{
  "name": "craftcms/craft",
  "description": "Craft CMS",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "project"
  ],
  "license": "MIT",
  "homepage": "https://craftcms.com/",
  "type": "project",
  "support": {
    "email": "support@craftcms.com",
    "issues": "https://github.com/craftcms/cms/issues",
    "forum": "https://craftcms.stackexchange.com/",
    "source": "https://github.com/craftcms/cms",
    "docs": "https://craftcms.com/docs",
    "rss": "https://craftcms.com/changelog.rss"
  },
  "require": {
    "craftcms/cms": "3.7.19.1",
    "vlucas/phpdotenv": "^2.4.0",
    "craftcms/redactor": "^2.1.0",
    "nystudio107/craft-minify": "^1.2.8",
    "verbb/field-manager": "^2.0",
    "nystudio107/craft-cookies": "^1.1",
    "danieltott/craft-sidebar-admin-links": "^1.1",
    "doublesecretagency/craft-matrixcolors": "^2.0",
    "craftcms/feed-me": "4.3.6",
    "dolphiq/sitemap": "1.3.1"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/",
      "modules\\toolkitmodule\\": "modules/toolkitmodule/src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.2.5"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@composer dump-autoload -o",
      "@php craft setup/welcome"
    ]
  }
}
